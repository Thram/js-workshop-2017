{
  "name": "js-workshop-2017",
  "version": "0.0.1",
  "description": "Modern JavaScript workshop.",
  "main": "dist/index.js",
  "scripts": {
    "example": "node examples/index.js",
    "commit": "git add . && git cz",
    "build": "webpack",
    "tape": "tape-watch -r babel-register __test__/*.test.js",
    "cover": "nyc npm run test:single",
    "lint": "eslint src --ext .jsx,.js",
    "lint:test": "eslint __test__ --ext .js",
    "test": "npm run lint && npm run tape -- -p | faucet",
    "test:single": "npm run lint && npm run tape -- --once -p | faucet",
    "start": "webpack-dev-server --host 0.0.0.0 --port 4000"
  },
  "author": "Thram <thramposo@gmail.com> (https://thram.github.io/)",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "faucet": "^0.0.1",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "husky": "^0.13.3",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.2.0",
    "rimraf": "^2.6.1",
    "tape": "^4.6.3",
    "tape-watch": "^2.3.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
